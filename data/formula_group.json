                "modulo": "Boletim de Medição Itens",
                "formula": "contrato.medicao_de_contrato_bm.contract_measurement_item.quantidade_acumulada_anterior + contrato.medicao_de_contrato_bm.contract_measurement_item.quantidade_medida",
